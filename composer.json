{
  "name": "adjeminpay/adjeminpay-sdk-php",
  "type": "library",
  "description": "SDK PHP to create transaction or get transaction status",
  "minimum-stability": "stable",
  "license": "MIT",
  "keywords": [
    "AdjeminPay",
    "api",
    "sdk",
    "php",
    "integration",
    "fintech",
    "mobile money"
  ],
  "homepage": "https://github.com/AdjeminPay/adjeminpay-sdk-php",
  "authors": [
    {
      "name": "Adjemin and contributors"
    }
  ],
  "require": {
    "php" : ">=7.4",
    "guzzlehttp/guzzle": "^6.3",
    "ext-json": "*"
  },
  "support": {
    "email": "support@adjeminpay.com"
  },

  "autoload": {
    "psr-4": {
      "AdjeminPay\\": "src/AdjeminPay/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.19"
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "7.4.0"
    }
  }
}